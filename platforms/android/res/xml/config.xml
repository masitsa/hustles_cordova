<?xml version='1.0' encoding='utf-8'?>
<widget id="com.janta" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <widget>
        <feature name="YoutubeVideoPlayer">
            <param name="android-package" value="com.bunkerpalace.cordova.YoutubeVideoPlayer" />
        </feature>
    </widget>
    <allow-intent href="market:*" />
    <name>Choto</name>
    <description>
        Make money by watching advertisements on your mobile phone
    </description>
    <author email="info@omnis.co.ke" href="https://www.omnis.co.ke">
        Omnis Limited
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
